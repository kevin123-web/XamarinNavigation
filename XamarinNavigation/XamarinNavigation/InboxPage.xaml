<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamarinNavigation.InboxPage"
             Title="Inbox">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Inbox Here" 
                   HorizontalOptions="Center" 
                   VerticalOptions="CenterAndExpand"/>
            <Button Text="Sent Items" Clicked="Button_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>